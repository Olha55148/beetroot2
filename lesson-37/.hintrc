{
  "extends": [
    "development"
  ],
  "hints": {
    "meta-charset-utf-8": "off",
    "axe/keyboard": [
      "default",
      {
        "nested-interactive": "off"
      }
    ],
    "axe/name-role-value": [
      "default",
      {
        "link-name": "off"
      }
    ],
    "meta-viewport": "off",
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "-webkit-text-size-adjust",
          "-moz-backface-visibility",
          "-webkit-tap-highlight-color"
        ]
      }
    ]
  }
}